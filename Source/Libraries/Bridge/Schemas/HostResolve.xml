<schema>
    <message name="HostServerInfo">
        <field name="guid" type="string"/>
        <field name="process" type="string"/>
        <field name="processId" type="uint64"/>
        <field name="application" type="string"/>
        <field name="api" type="string"/>
    </message>
    <message name="HostDiscovery">
        <field name="infos" type="stream"/>
    </message>
    <message name="HostResolved">
        <field name="accepted" type="bool"/>
    </message>
    <message name="HostConnected">
        <field name="accepted" type="bool"/>
    </message>
</schema>

<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Studio"
             xmlns:idc="using:Dock.Avalonia.Controls"
             xmlns:controls="using:Studio.Views.Controls"
             xmlns:core="using:Dock.Model.Core"
             x:Class="Studio.App">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <StyleInclude Source="avares://Studio/Icons.axaml" />
        <StyleInclude Source="avares://Studio/Themes/WorkspacePropertyView.axaml" />
    </Application.Styles>

    <Application.Resources>
        <controls:ContextMenuView x:Key="ReactiveContextMenu"/>
    </Application.Resources>
</Application>